{
    "name": "imnicy/framework",
    "description": "A Simple PHP Framework",
    "license": "MIT",
    "homepage": "https://framework.imnicy.com",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-pdo": "*",
        "ext-json": "*",
        "catfan/medoo": "^2.1",
        "dflydev/fig-cookies": "^3.0",
        "imnicy/container": "^1.0",
        "imnicy/support": "^1.0",
        "jasongrimes/paginator": "^1.0",
        "league/event": "^2.2",
        "league/flysystem": "^2.0",
        "monolog/monolog": "^2.2",
        "phpfastcache/phpfastcache": "^8.0",
        "rakit/validation": "^1.4",
        "slim/psr7": "^1.0",
        "slim/slim": "^4.0",
        "twig/twig": "^3.0",
        "josegonzalez/dotenv": "^3.2",
        "psr/log": "^1.0",
        "lcobucci/jwt": "^4.2",
        "nesbot/carbon": "^2.62"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "tomasfejfar/enhanced-dump": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Nicy\\Framework\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nicy\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
